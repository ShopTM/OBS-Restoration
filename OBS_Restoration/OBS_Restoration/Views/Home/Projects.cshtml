
@{
  ViewBag.Title = "Projects";
}
@section Scripts {
  <script src="~/Content/Js/Custom/projects.js"></script>
  <script src="~/Content/Js/isotope-docs.min.js"></script>
  <link href="~/Content/Css/magnific-popup.css" rel="stylesheet" />
}
@section Styles {
  <link href="~/Content/Css/Custom/project.css" rel="stylesheet" />
  <script src="~/Content/Js/jquery.magnific-popup.min.js"></script>
}
<section class="container-fluid">
  <section class="row">
    <section class="col-lg-12 project-bg">
      <h2><span>Look</span> at your best projects</h2>
    </section> <!--project-bg-text-->
  </section> <!--project-bg-->


  <section class="project-filter ">
    <section class="row">
      <section class="col-lg-12 project-filter-links">
        <a href="#" data-filter="">All projects</a>
      </section>  <!--project-filter-links-->
    </section>  <!--row-->
  </section> <!--container-filter-->

  <section class="project-container">
  </section> <!--project-container-->
</section> <!--container-fluid-->
<!--TAMPLATE-->
<template id="templateTab">
  <a class="tab" href="#" data-filter=""></a>
</template>
<template id="templateImg">
  <section class="project-container-img">
    <img src="" />
  </section>
</template>

