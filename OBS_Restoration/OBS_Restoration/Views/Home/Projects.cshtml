@{
    ViewBag.Title = "Projects";
}
@section Scripts {

    <script src="~/Content/Js/Custom/projects.js"></script>
    <script src="~/Content/Js/isotope-docs.min.js"></script>
}
@section Styles {
    <link href="~/Content/Css/Custom/project.css" rel="stylesheet" />
}
<section class="container-fluid">
    <section class="row">
        <section class="col-lg-12 project-bg">
            <section class="slide-text">
                <h2><span>Look</span> at your best projects</h2>
            </section>
        </section> <!--project-bg-text-->
    </section> <!--project-bg-->
    <section class="errorProject">
        <svg class="success">
            <use xlink:href="../../Content/Svg/spites/symbol/sprite.svg#warning"></use>
        </svg>
        <p class="errorProjectMessage"></p>
    </section>
    <section class="grid">
        <section class="button-group filters-button-group">
            <button class="button is-checked" data-filter="*">All projects</button>
        </section>
        <section class="images-group"></section> <!--project-container-->
    </section>
</section> <!--container-fluid-->


<!--TAMPLATES-->
<template id="tabTemplate">
    <button class="button"></button>
</template>
<template id="imgTemplate">
    <img src="" alt="Project Image" class="element-item transition" data-category="transition" />
</template>
